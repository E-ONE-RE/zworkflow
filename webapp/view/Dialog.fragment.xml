<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:l="sap.ui.layout"
   xmlns:core="sap.ui.core"
   	xmlns:html="http://www.w3.org/1999/xhtml">
	
	 
	<Dialog 
	title="Users" 
	contentHeight="auto" 
	contentWidth="auto">
		<content>
			<Input
				type="Text"
				placeholder="Enter a user surname..."
				showSuggestion="true"
				suggestionItems="{/UserSet}">
				<suggestionItems>
					<core:Item key="{Uname}" text="{Name2} {Name1}" />
				</suggestionItems>
			</Input>
		</content>
		<beginButton>
			<Button text="Ok" press="onAccept"/>
		</beginButton>
	    <endButton>
		 	<Button text="Cancel" press="onCancel"/>
		</endButton>
		</Dialog>
		
	</core:FragmentDefinition>